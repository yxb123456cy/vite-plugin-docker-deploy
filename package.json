{
  "name": "@qy-utils/vite-plugin-docker-deploy",
  "version": "1.5.1",
  "packageManager": "pnpm@10.14.0",
  "description": "一个Vite插件",
  "author": "yxb123456cy <2186256471@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/yxb123456cy/vite-plugin-docker-deploy",
  "repository": {
    "type": "git",
    "url": "https://github.com/yxb123456cy/vite-plugin-docker-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/yxb123456cy/vite-plugin-docker-deploy/issues"
  },
  "keywords": [
    "vite",
    "typescript",
    "vite-plugin",
    "plugn",
    "deploy",
    "docker",
    "docker-deploy",
    "vite-docker",
    "docker-deploy-plugin",
    "vite-docker-deploy",
    "docker-deploy-vite-plugin",
    "vite-plugin-docker-deploy"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:prod": "cross-env NODE_ENV=production tsup",
    "commit": "git-cz",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "husky",
    "test": "vitest",
    "release": "standard-version",
    "publish:all": "npm version patch && git push --follow-tags",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vite": "^7.1.4"
  },
  "dependencies": {
    "@qy-utils/vite-plugin-docker-deploy": "^1.2.0",
    "@types/node": "^24.3.0",
    "archiver": "^7.0.1",
    "node-ssh": "^13.2.1",
    "ssh2": "^1.17.0",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/archiver": "^6.0.3",
    "@types/ssh2": "^1.15.5",
    "cross-env": "^10.0.0",
    "cz-git": "^1.12.0",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.4",
    "vitepress": "^1.6.4"
  }
}
